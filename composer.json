{
    "name": "ixocreate/scheduler-package",
    "description": "IXOCREATE Scheduler Package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "cocur/background-process": "^0.7.0",
        "dragonmantank/cron-expression": "^2.1",
        "ixocreate/application": "dev-consolidate",
        "symfony/lock": "^4.1"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0",
        "phpunit/phpunit": "^7.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Scheduler\\": "src/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
